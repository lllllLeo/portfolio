package main;

public class main {
	public static void main(String[] args) {
		
	int 숫자후보[] = new int[3];
	
		for(int i=0; i<숫자후보.length; i++) {
			숫자후보[i] = (int)(Math.random() * 9);
			for(int j=0; j<i; j++) {
				if(숫자후보[i]== 숫자후보[j]) {
					i--;
					break;
				}
			}
		}
		System.out.println("배열길이는 "+숫자후보.length);
	}
}
